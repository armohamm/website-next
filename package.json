{
  "name": "website-next",
  "version": "1.0.0",
  "private": true,
  "description": "Red Badger's website",
  "main": "build/server",
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "webpack --config config/client.webpack.config.babel.js",
    "build:server": "webpack --config config/server.webpack.config.babel.js",
    "build:server-tests": "webpack --config config/server-tests.webpack.config.babel.js",
    "clean": "rimraf build",
    "lint": "eslint --cache .",
    "start": "node build/server",
    "test:client": "karma start config/karma.config.babel.js",
    "test:e2e": "wdio config/wdio.config.js",
    "test:server": "mocha ./build/test/index.js",
    "watch:client": "HMR=true webpack-dev-server --config config/client.webpack.config.babel.js --content-base build/client --hot --inline --output-public-path http://localhost:8080/ ",
    "watch:server": "HMR=true webpack --config config/server.webpack.config.babel.js --watch --output-public-path http://localhost:8080/ & HMR=true nodemon --watch build/server build/server",
    "watch:server-tests": "webpack --config config/server-tests.webpack.config.babel.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/website-next.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/redbadger/website-next/issues"
  },
  "homepage": "https://github.com/redbadger/website-next",
  "pre-push": [
    "lint",
    "build:server-tests",
    "test:server"
  ],
  "dependencies": {
    "classnames": "^2.2.1",
    "express": "^4.13.3",
    "lodash": "^4.0.0",
    "moment": "^2.10.6",
    "node-fetch": "^1.3.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nodemon": "1.8.1",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "postcss-cssnext": "^2.3.0",
    "postcss-discard-duplicates": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "pre-push": "0.1.1",
    "react-addons-test-utils": "^0.14.5",
    "react-shallow-testutils": "^0.7.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "2.5.0",
    "sinon": "^1.17.2",
    "url-loader": "^0.5.7",
    "webdriverio": "^3.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "5.3.0"
  }
}
