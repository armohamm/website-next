{
  "name": "website-next",
  "version": "1.0.0",
  "private": true,
  "description": "Red Badger's website",
  "main": "build/server",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf build",
    "lint": "eslint --cache .",
    "security:check": "./node_modules/.bin/nsp check",
    "start": "node build/server",
    "test": "npm run test:server && npm run test:client && npm run test:e2e",
    "test:client": "karma start spec/karma.config.js",
    "test:e2e": "wdio spec/wdio.config.js",
    "test:server": "mocha --compilers js:babel-core/register --require spec/mocha-helper.js --recursive --reporter spec $(find src -name *spec.js)",
    "watch:client": "HMR=true webpack-dev-server --config config/client.webpack.config.babel.js --content-base build/client --hot --inline --output-public-path http://localhost:8080/ ",
    "watch:server": "HMR=true webpack --config config/server.webpack.config.babel.js --watch --output-public-path http://localhost:8080/ & HMR=true nodemon --watch build/server build/server",
    "watch:server-tests": "webpack --config config/server-tests.webpack.config.babel.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/website-next.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/redbadger/website-next/issues"
  },
  "homepage": "https://github.com/redbadger/website-next",
  "pre-push": [
    "lint",
    "test:server"
  ],
  "dependencies": {
    "classnames": "^2.2.1",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "html-to-react": "^0.1.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.0.0",
    "moment": "^2.10.6",
    "node-fetch": "^1.3.3",
    "param-case": "^1.1.2",
    "query-string": "^3.0.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "3.0.5",
    "redux-devtools": "3.0.1",
    "redux-thunk": "^1.0.3",
    "serialize-javascript": "^1.1.2",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015-webpack": "^6.3.13",
    "babel-preset-node5": "^10.3.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-hook": "^0.2.0",
    "nodemon": "1.8.1",
    "nsp": "^2.2.0",
    "phantomjs": "^2.1.3",
    "phantomjs-polyfill": "0.0.1",
    "postcss-cssnext": "^2.3.0",
    "postcss-discard-duplicates": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "pre-push": "0.1.1",
    "react-addons-test-utils": "^0.14.5",
    "react-shallow-testutils": "^0.7.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "2.5.0",
    "sinon": "^2.0.0-pre",
    "url-loader": "^0.5.7",
    "webdriverio": "^3.4.0",
    "webpack": "^2.0.6-beta",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "5.5.0"
  }
}
