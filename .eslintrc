{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "document": false,
    "window": false
  },
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "rules": {
    "object-curly-spacing": [2, "always"],
    "no-console": 0,
    "react/jsx-uses-react": 1
  }
}
